{
  "name": "nr-send-test-results",
  "description": "Send test results to New Relic",
  "private": true,
  "version": "0.0.0",
  "main": "lib/main.js",
  "scripts": {
    "format": "prettier --write '**/*.ts'",
    "format-check": "prettier --check '**/*.ts'",
    "lint": "eslint --fix src/**/*.ts",
    "lint-check": "eslint src/**/*.ts",
    "build": "tsc",
    "package": "ncc build --source-map --license licenses.txt",
    "all": "npm run format && npm run lint && npm run build && npm run package",
    "clean": "rm lib/* && rm -rf dist/",
    "clean-all": "rm -rf node_modules && rm lib/* && rm -rf dist/"
  },
  "keywords": [
    "github",
    "action",
    "new-relic",
    "test-results",
    "logs"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pavelsaman/nr-send-test-results.git"
  },
  "author": "Pavel Saman",
  "license": "MIT",
  "dependencies": {
    "@actions/artifact": "^1.1.0",
    "@actions/core": "^1.10.0",
    "@actions/github": "^5.1.1",
    "axios": "^1.1.3",
    "moment": "^2.29.4"
  },
  "devDependencies": {
    "@types/node": "^18.11.9",
    "@typescript-eslint/parser": "^5.42.0",
    "@vercel/ncc": "^0.34.0",
    "eslint": "^8.26.0",
    "eslint-plugin-github": "^4.4.0",
    "prettier": "^2.7.1",
    "typescript": "^4.8.4"
  }
}
